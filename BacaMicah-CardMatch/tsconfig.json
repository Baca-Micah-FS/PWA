{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./src/types"] //Had to add typeRoots to point to the assets.d.ts to stop the linting error on the image import in ts
  },
  "include": ["src/**/*"]
}
